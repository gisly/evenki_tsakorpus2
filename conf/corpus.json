{
  "corpus_name": "evenki",
  "citation": "О. А. Казакевич, Е. Л. Клячко, Н. К. Митрофанова. Корпус эвенкийского языка (https://minlang.iling-ran.ru/corpora/evenki). Лицензия: https://creativecommons.org/licenses/by-sa/4.0/",
  "main_tiers": ["sentFon"],
  "aligned_tiers": ["rus", "evCyr"],
  "analysis_tiers": {"fonWord":"word", "fonConcat":"parts", "glConcat":"gloss"},
  "tier_languages": {"sentFon" : "evenkiTr", "rus":"russian",
    "fonConcat":"evenkiTr", "fonWord":"russian", "evCyr" : "evenki"},
  "input_format": "json-gzip",
  "gzip": "true",
  "meta_files_dir": "false",
  "meta_files_ext": "false",
  "meta_filename": "meta.csv",
  "debug": false,
  "media": true,
  "video": true,
  "media_length": 60,
  "max_context_expand": 10,
  "max_distance_filter": 200000,
  "max_docs_retrieve": 9999,
  "max_words_in_sentence": 40,
  "query_timeout": 60,
  "year_sort_enabled": true,
  "viewable_meta": [
    "filename",
    "author",
    "year",
    "recplace",
    "title",
    "genre",
    "year_from",
    "year_to",
	"site_url",
    "dialect",
    "dialect_group"
  ],
  "meta_fields": [
    "filename",
    "author",
    "year",
    "recplace",
    "title",
    "genre",
    "year_from",
    "year_to",
	"site_url",
    "gender",
    "dialect",
    "dialect_group"
  ],
  "meta_files_case_sensitive": "false",
  "sentence_meta": [
    "age",
    "gender"
  ],
  "search_meta": {
    "columns": [
      [
        {"type": "input_text", "name": "author", "placeholder": "Author"},
		{"type": "input_text", "name": "recplace", "placeholder": "recplace"},
        {"type": "input_text", "name": "title", "placeholder": "Title"},
        {"type": "separator"},
        {"type": "input_number", "name": "year_from", "placeholder": "Year: from"},
        {"type": "input_number", "name": "year_to", "placeholder": "Year: to"}
      ],
      [
        {"type": "input_text", "name": "genre", "placeholder": "Genre"},
        {"type": "option", "name": "genre", "value": "folklore", "text": "folklore"},
        {"type": "option", "name": "genre", "value": "lifestory", "text": "lifestory"}
      ],
      [
        {"type": "input_text", "name": "dialect", "placeholder": "Dialect"},
        {"type": "option", "name": "dialect", "value": "Ilimpeya", "text": "Ilimpeya"},
        {"type": "option", "name": "dialect", "value": "Yerbogachyon", "text": "Yerbogachyon"},
        {"type": "option", "name": "dialect", "value": "Sym", "text": "Sym"},
        {"type": "option", "name": "dialect", "value": "Podkamennaya Tunguska", "text": "Podkamennaya Tunguska"},
        {"type": "option", "name": "dialect", "value": "Vitim-Tungir-Olyokma", "text": "Vitim-Tungir-Olyokma"},
        {"type": "option", "name": "dialect", "value": "Tokma-Upper Lena", "text": "Tokma-Upper Lena"},
        {"type": "option", "name": "dialect", "value": "Upper Aldan", "text": "Upper Aldan"},
        {"type": "option", "name": "dialect", "value": "Uchur-Zeya", "text": "Uchur-Zeya"},
        {"type": "option", "name": "dialect", "value": "Totta", "text": "Totta"},
        {"type": "option", "name": "dialect", "value": "Tugur-Chumikan", "text": "Tugur-Chumikan"}
      ]
      ,
      [
        {"type": "input_text", "name": "dialect_group", "placeholder": "Dialect group"},
        {"type": "option", "name": "dialect_group", "value": "Northern", "text": "Northern"},
        {"type": "option", "name": "dialect_group", "value": "Southern", "text": "Southern"},
        {"type": "option", "name": "dialect_group", "value": "Eastern", "text": "Eastern"}
      ]
    ],
    "stat_options": [
      "genre",
      "author",
      "title",
      "year",
      "recplace",
      "dialect",
      "dialect_group"
    ]
  },
  "word_fields":
  [
    "trans_ru",
    "trans_en"
  ],
  "default_values": {"sent_meta_speaker_type": "native"},
  "sentence_meta_values": {
    "speaker_type": ["native", "linguist"]
  },
  "keyboards": {
    "evenki": "evenki",
    "russian": "russian"
  },
  "generate_dictionary": true,
  "fulltext_view_enabled": true,
  "use_nonpersistent_fulltext_id": true,
  "languages": ["evenkiTr", "evenki", "russian"],
  "transliterations": ["original", "IPA"],
  "input_methods": ["normal"],
  "interface_languages": [
    "ru", "en"
  ],
  "all_language_search_enabled": true,
  "gloss_search_enabled": true,
  "ambiguous_analyses": true,
  "json_indent": -1,
  "lang_props": {},
  "media_dir": "corpus/evenki/test_media"

}