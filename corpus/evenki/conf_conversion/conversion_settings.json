{
  "corpus_name": "evenki",
  "corpus_dir": "corpus",

  "error_log": "error.log",

  "meta_files_case_sensitive": true,
   "languages": ["evenkiTr", "evenki", "russian"],
  "json_indent": -1,
  "gzip": true,
  "convert_quotes": true,
  "left_quot_mark": "«",
  "right_quot_mark": "»",
  "sent_end_punc": "(?:[.!?;;]+(?:[)\\]}>/»]|$)|\\\\n)",
  "sent_start": "^[A-ZÄÖÜА-ЯЁӤӦӴӞӜ0-9]",
  "newline_ends_sent": true,
  "abbreviations": [],
  "media_length": 60,
   "main_tiers": ["sentFon"],
  "aligned_tiers": ["rus", "evCyr"],
  "analysis_tiers": {"fonWord":"word", "fonConcat":"parts", "glConcat":"gloss"},
  "tier_languages": {"sentFon" : "evenkiTr", "rus":"russian",
    "fonConcat":"evenkiTr", "fonWord":"russian", "evCyr" : "evenki"},
  "meta_files_dir": "false",
  "meta_files_ext": "false",
  "meta_filename": "meta.csv",
  "meta_fields": [
    "filename",
    "author",
    "year",
    "recplace",
    "title",
    "genre",
    "year_from",
    "year_to",
	"site_url",
    "gender",
    "dialect",
    "dialect_group"
  ],


  "transliterations": ["original", "IPA"],
  "input_methods": ["normal"],
  "interface_languages": [
    "ru", "en"
  ],
    "media_dir": "corpus/evenki/test_media"
}